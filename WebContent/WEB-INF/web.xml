<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<servlet>
		<servlet-name>UploadCSV</servlet-name>
		<servlet-class>com.demo.UploadCSV</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UploadCSV</servlet-name>
		<url-pattern>/UploadCSV</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>LoginServ</servlet-name>
		<servlet-class>com.demo.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginServ</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddUser</servlet-name>
		<servlet-class>com.demo.AddUsers</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/AddUser</url-pattern>
	</servlet-mapping>

	
	


</web-app>